{
    "name": "smart-page-builder",
    "version": "1.0.0",
    "description": "Smart Page Builder WordPress Plugin",
    "main": "index.js",
    "scripts": {
        "build": "webpack --mode=production",
        "dev": "webpack --mode=development --watch",
        "lint:js": "eslint admin/js public/js --ext .js",
        "lint:css": "stylelint admin/css/**/*.css public/css/**/*.css",
        "lint": "npm run lint:js && npm run lint:css",
        "fix:js": "eslint admin/js public/js --ext .js --fix",
        "fix:css": "stylelint admin/css/**/*.css public/css/**/*.css --fix",
        "fix": "npm run fix:js && npm run fix:css",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "keywords": [
        "wordpress",
        "plugin",
        "ai",
        "content-generation",
        "seo"
    ],
    "author": "John Housholder",
    "license": "GPL-2.0-or-later",
    "devDependencies": {
        "webpack": "^5.88.0",
        "webpack-cli": "^5.1.0",
        "css-loader": "^6.8.0",
        "mini-css-extract-plugin": "^2.7.0",
        "sass": "^1.63.0",
        "sass-loader": "^13.3.0",
        "eslint": "^8.44.0",
        "eslint-config-wordpress": "^2.0.0",
        "stylelint": "^15.10.0",
        "stylelint-config-wordpress": "^17.0.0",
        "jest": "^29.6.0",
        "babel-loader": "^9.1.0",
        "@babel/core": "^7.22.0",
        "@babel/preset-env": "^7.22.0"
    },
    "dependencies": {},
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "jest": {
        "testEnvironment": "jsdom",
        "testMatch": [
            "**/tests/**/*.test.js"
        ]
    }
}
